						"id": "97324442-114d-484d-b883-ebbc9c04bc3f",
						"exec": [
							"const contohSchema = {\r",
							"    \"$schema\": \"http://json-schema.org/draft-07/schema#\",\r",
							"    \"type\": \"object\",\r",
							"    \"properties\": {\r",
							"        \"page\": {\"type\": \"number\" },\r",
							"        \"per_page\": {\"type\": \"number\" },\r",
							"        \"total\": {\"type\": \"number\" },\r",
							"        \"total_pages\": {\"type\": \"number\" },\r",
							"        \"data\": {\r",
							"            \"type\": \"array\",\r",
							"            \"properties\": {\r",
							"                \"id\": { \"type\": \"number\" },\r",
							"                \"email\": { \"type\": \"string\", \"format\": \"email\" },\r",
							"                \"first_name\": { \"type\": \"string\" },\r",
							"                \"last_name\": { \"type\": \"string\" },\r",
							"                \"avatar\": { \"type\": \"string\" },\r",
							"            },\r",
							"            \"required\": [\"id\", \"email\", \"first_name\", \"last_name\", \"avatar\"]\r",
							"        },\r",
							"        \"support\": {\r",
							"            \"type\": \"object\",\r",
							"            \"properties\": {\r",
							"                \"url\": { \"type\": \"string\" },\r",
							"                \"text\": { \"type\": \"string\" }\r",
							"            },\r",
							"            \"required\": [\"url\", \"text\"]\r",
							"        }\r",
							"    },\r",
							"    \"required\": [\"page\", \"per_page\", \"total\", \"total_pages\", \"data\", \"support\"]\r",
							"}\r",
							"\r",
							"pm.environment.get(\"variable_key\");\r",
							"pm.test(\"Response berisi Data JSON yang valid\", () => {\r",
							"    pm.response.to.have.jsonSchema(contohSchema);\r",
							"})"
						],
